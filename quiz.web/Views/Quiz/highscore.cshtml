@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<IEnumerable<quiz.web.Models.Submission>>

@{
    Layout = "_Layout.cshtml";
}

<table class="table table-striped">
    <tr>
        <th>Name</th>
        <th>Score</th>
    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.Name</td>
            <td>@s.Score</td>
        </tr>
    }
</table>